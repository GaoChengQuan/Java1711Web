    <servlet-class>com.situ.day28.ThreadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ThreadServlet</servlet-name>
    <url-pattern>/thread</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PathServlet</servlet-name>
    <servlet-class>com.situ.day28.PathServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PathServlet</servlet-name>
    <url-pattern>/path</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.situ.day28.LoginServlet</servlet-class>
